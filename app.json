{
  "expo": {
    "name": "Shelfze",
    "slug": "shelfze",
    "version": "1.0.0",
    "owner": "m-ai-doo",
    "orientation": "portrait",
    "icon": "assets/icon.png",
    "scheme": "shelfze",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shelfze.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Shelfze needs access to your camera to scan food items and expiry dates.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.747247644525-bmcc1cc6uq06kdd8aiobr0d2iu0h8s53"
            ]
          }
        ],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ],
        "LSApplicationQueriesSchemes": [
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.shelfze.app",
      "adaptiveIcon": {
        "foregroundImage": "assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "android.permission.INTERNET"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "assets/favicon.png"
    },
    "plugins": [],
    "sdkVersion": "54.0.0",
    "extra": {
      "googleIosClientId": "747247644525-bmcc1cc6uq06kdd8aiobr0d2iu0h8s53.apps.googleusercontent.com",
      "googleAndroidClientId": "747247644525-hpg8l19sug00gqmk33s6vfe9442b7t15.apps.googleusercontent.com",
      "googleWebClientId": "747247644525-q07j97skv56o7b5vp3vt30bbh6ges476.apps.googleusercontent.com",
      "eas": {
        "projectId": "230cf138-b663-453b-808a-e98fa066b545"
      }
    }
  }
}
